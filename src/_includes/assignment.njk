---
layout: base.njk
---
<article class="container is-max-desktop">
    <header class="block">
        <h1 class="title">{{ title }}</h1>
        <p class="subtitle">Due: <time datetime="{{ page.date | htmlDateString }}">{{ page.date | readableDate }}</time></p>
    </header>
    <div class="content">
        {{ content | safe }}
    </div>
</article>
